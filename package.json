{
  "name": "clean-demo",
  "version": "0.1.0",
  "description": "Demo clean architecture in React with SSR",
  "main": "index.js",
  "license": "MIT",
  "private": true,
  "scripts": {
    "dev:client": "webpack serve --host 0.0.0.0 --port ${DEV_PORT:=4000} --progress --config webpack.config.js",
