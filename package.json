    "lint:js": "eslint ./app ./client ./server --ignore-path .gitignore --ext .jsx,.js",
    "lint:style": "stylelint ./app/**/*.scss --formatter verbose",
    "lint-staged": "lint-staged"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.15.0",
